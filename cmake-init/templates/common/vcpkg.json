{
  "name": "{= name =}",
  "version-semver": "{= version =}",
  "dependencies": [
    {
      "name": "{% if c %}json-c{% else %}fmt{% end %}",
      "version>=": "{% if c %}2022-06-26{% else %}9.1.0{% end %}"
    }{% if c and exe %},
    {
      "name": "hedley",
      "version>=": "15"
    }{% end %}
  ],
  "default-features": [],
  "features": {
    "test": {
      "description": "Dependencies for testing",
      "dependencies": [
        {
          "name": "catch2",
          "version>=": {% if catch3 %}"3.1.1#1"{% else %}"2.13.9#1"{% end %}
        }
      ]
    }
  },{% if not catch3 %}
  "overrides": [
    { "name": "catch2", "version": "2.13.9#1" }
  ],{% end %}
  "builtin-baseline": "94ce0dab56f4d8ba6bd631ba59ed682b02d45c46"
}
